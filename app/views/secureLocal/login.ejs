<% include  ../partials/header %>
	 
 
	 
	<h3><%= title %></h3>


<div class="container">
        <div class="row">
               


        <form class="form-horizontal col-sm-10"  action="/loginLocal" method="post" novalidate>

            <div class="form-group">
                <label class="col-sm-2 control-label" for="name">Name</label>
                <div class="col-sm-6">

                    
                    <input required name="username" id="username" class="form-control" type="text" placeholder="Your username (test)" />
                </div>
            </div>



            <div class="form-group">
                <label class="col-sm-2 control-label" for="password">Password</label>
                <div class="col-sm-6">

                    
                    <input required name="password" id="password" class="form-control" type="text" placeholder="Your password (test)" />
                </div>

                 <div class="col-sm-4">
                    <input  type="submit"  value="Submit" class="btn   btn-primary"> 

                 </div>


            </div>
            <% if (locals.sendToPath) { %>
                  <input type="hidden" value=<%= sendToPath %> />
                
            <% } %>
            
        </form>

</div>
</div>



 <% include  ../partials/footer %>

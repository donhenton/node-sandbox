<% include  ../partials/header %>
	 
 
	 
	<h3><%= title %></h3>


<div class="container">
        <div class="row">
               


        <form class="form-horizontal col-sm-10"  action="/loginLocal" method="post" novalidate>

            <div class="form-group">
                <label class="col-sm-2 control-label" for="name">Name</label>
                <div class="col-sm-6">

                    
                    <input required name="username" id="username" class="form-control" type="text" placeholder="Your username" />
                </div>
            </div>



            <div class="form-group">
                <label class="col-sm-2 control-label" for="password">Password</label>
                <div class="col-sm-6">

                    
                    <input required name="password" id="password" class="form-control" type="text" placeholder="Your password" />
                </div>

                 <div class="col-sm-4">
                    <input  type="submit"  value="Submit" class="btn   btn-primary"> 

                 </div>


            </div>
            <% if (locals.sendToPath) { %>
                  <input type="hidden"  name="sendToPath" value=<%= sendToPath %> />
                
            <% } %>
            
        </form>

    </div>
    <p>&nbsp;</p>
    <div class="row">
        <div class="col-sm-6">
            <table class="table table-striped">
            <thead>
            <tr><th>User Name</th><th>Password</th><th>Roles</th></tr>
            </thead>
            <tbody>
                <tr><td>test</td><td>test</td><td>alpha,beta</td></tr>
                <tr><td>user2</td><td>user2</td><td>alpha,beta,gamma</td></tr>
                <tr><td>noroles</td><td>noroles</td><td>[]</td></tr>
            </tbody>

            </table>
        </div>


    </div>
</div>



 <% include  ../partials/footer %>
